{
  "name": "operations-portal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "start-server-and-test \"start:server\" http://localhost:8080 \"ng serve\"",
    "start:server": "cross-env DIST_DIR=./dist/out node ./server/server.js",
    "build": "ng build",
    "lint": "ng lint",
    "test": "yarn test:unit && yarn test:e2e",
    "pretest": "rimraf .nyc_output || true",
    "posttest": "yarn run report:combined",
    "test:pact": "jest -c jest.config.pact.js --runInBand",
    "pact:publish": "pact-broker publish ./pact/pacts --consumer-app-version=${PACT_VERSION}",
    "test:unit": "jest --verbose --coverage",
    "test:unitband": "jest --verbose --coverage --runInBand",
    "test:unitthrottled": "jest --verbose --coverage --maxWorkers=4",
    "test:e2e": "start-server-and-test start http://localhost:4200 \"cypress run\"",
    "test:e2e:local": "start-server-and-test start http://localhost:4200 \"cypress open\"",
    "test:e2e:ci": "start-server-and-test 'cross-env DIST_DIR=$PWD/dist/out node ./server/server.js' http://localhost:8080 'cross-env CYPRESS_BASE_URL=http://localhost:8080 cypress run'",
    "mkdir:reports": "mkdir reports || true",
    "precopy:reports": "yarn run mkdir:reports",
    "copy:reports": "ncp reports/coverage/cypress/coverage-final.json reports/from-cypress.json && ncp reports/coverage/jest/coverage-final.json reports/from-jest.json",
    "precombine:reports": "yarn run copy:reports && mkdir .nyc_output || true",
    "combine:reports": "yarn nyc merge reports && mv coverage.json .nyc_output/out.json",
    "prereport:combined": "yarn combine:reports",
    "report:combined": "yarn nyc report --reporter html --reporter text --report-dir reports/coverage/combined",
    "postreport:combined": "rimraf reports/from-cypress.json && rimraf reports/from-jest.json || true"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.8",
    "@angular/cdk": "^10.1.2",
    "@angular/common": "^10.0.8",
    "@angular/compiler": "^10.0.8",
    "@angular/core": "^10.0.8",
    "@angular/forms": "^10.0.8",
    "@angular/localize": "^10.0.8",
    "@angular/material": "^10.1.2",
    "@angular/platform-browser": "^10.0.8",
    "@angular/platform-browser-dynamic": "^10.0.8",
    "@angular/router": "^10.0.8",
    "@greensill/gds-ui": "^0.11.49-beta",
    "@ng-stack/forms": "^2.2.2",
    "@swimlane/ngx-datatable": "^18.0.0",
    "@types/lodash-es": "^4.17.3",
    "@zxing/browser": "^0.0.9",
    "@zxing/library": "^0.18.6",
    "@zxing/ngx-scanner": "^3.1.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "file-saver": "^2.0.2",
    "helmet": "^4.0.0",
    "jwt-decode": "^3.0.0-beta.2",
    "lodash-es": "^4.17.15",
    "newrelic": "^6.13.0",
    "ngx-markdown": "^11.0.1",
    "ngx-mat-select-search": "^3.1.4",
    "rxjs": "^6.6.2",
    "tslib": "^2.0.0",
    "utility-types": "^3.10.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.5",
    "@angular/cli": "^10.0.5",
    "@angular/compiler-cli": "^10.0.8",
    "@angular/language-service": "^10.0.8",
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@cypress/code-coverage": "^3.8.1",
    "@cypress/webpack-preprocessor": "^5.4.2",
    "@pact-foundation/pact": "^9.14.0",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.9",
    "@types/node": "^12.12.54",
    "babel-loader": "^8.1.0",
    "codelyzer": "^6.0.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "cypress": "^4.12.1",
    "cypress-keycloak-commands": "^1.2.0",
    "husky": "^4.2.5",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-spec-reporter": "^5.0.2",
    "jest": "^26.2.2",
    "jest-pact": "^0.8.1",
    "jest-preset-angular": "^8.2.1",
    "keycloak-angular": "^8.0.1",
    "keycloak-js": "^11.0.0",
    "ncp": "^2.0.0",
    "ng-mocks": "^10.1.1",
    "ngx-build-plus": "^10.1.1",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.11.2",
    "ts-loader": "7.0.4",
    "ts-node": "^8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~3.9.7",
    "webpack": "^4.18.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && ng lint && yarn test"
    }
  }
}
